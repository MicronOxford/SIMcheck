# Name: SIMcheck
# Author: Graeme Ball
# Version: 0.5
# Date: 2013/01/22
# Requires: ImageJ 
# Generate the jar file using: "jar cvfM SIMcheck_.jar *"

Plugins>SIMcheck>, "Run SIMcheck", SIMcheck.SIMcheck_
Plugins>SIMcheck>raw data, "Angle Difference", SIMcheck.Raw_Angle_Difference
Plugins>SIMcheck>raw data, "Intensity Profiles", SIMcheck.Raw_intensity
Plugins>SIMcheck>raw data, "Fourier Plots", SIMcheck.Raw_Fourier
Plugins>SIMcheck>raw data, "Modulation Contrast", SIMcheck.Raw_ModContrast
Plugins>SIMcheck>reconstructed data, "SIR Histogram", SIMcheck.SIR_histogram
Plugins>SIMcheck>reconstructed data, "SIR Z Variation", SIMcheck.SIR_Z_variation
Plugins>SIMcheck>reconstructed data, "SIR Fourier Plot ", SIMcheck.SIR_Fourier
Plugins>SIMcheck>reconstructed data, "SIR Mod Contrast Map", SIMcheck.SIR_ModContrastMap
Plugins>SIMcheck>system calibration, "Check Phases", SIMcheck.Cal_Phases
Plugins>SIMcheck>utilities, "FFT stack (2D)", SIMcheck.Util_FFTstack
Plugins>SIMcheck>utilities, "Raw Data to Pseudo-Wide-Field", SIMcheck.Util_SI2WF
Plugins>SIMcheck>utilities, "SIM Format Converter", SIMcheck.Util_formats
Image>Lookup Tables, "SIMcheck MCNR", MCNR.lut
Image>Lookup Tables, "SIMcheck Fourier", SIMcheckFourier.lut
