# Name: SIMcheck
# Author: Graeme Ball
# Version: 0.9.3-SNAPSHOT
# Date: 21/07/2014
# Requires: ImageJ 
# Generate the jar file using: "jar cvfM SIMcheck_.jar *"

Plugins>SIMcheck>, "Run SIMcheck", SIMcheck.SIMcheck_
Plugins>SIMcheck>raw data, "Motion Check", SIMcheck.Raw_MotionCheck
Plugins>SIMcheck>raw data, "Intensity Profiles", SIMcheck.Raw_IntensityProfiles
Plugins>SIMcheck>raw data, "Fourier Projections", SIMcheck.Raw_FourierProjections
Plugins>SIMcheck>raw data, "Modulation Contrast", SIMcheck.Raw_ModulationContrast
Plugins>SIMcheck>reconstructed data, "Intensity Histograms", SIMcheck.Rec_IntensityHistograms
Plugins>SIMcheck>reconstructed data, "Sperical Aberration Mismatch", SIMcheck.Rec_SAMismatch
Plugins>SIMcheck>reconstructed data, "Fourier Plots", SIMcheck.Rec_FourierPlots
Plugins>SIMcheck>reconstructed data, "Modulation Contrast Map", SIMcheck.Rec_ModContrastMap
Plugins>SIMcheck>system calibration, "Phase Steps", SIMcheck.Cal_PhaseSteps
Plugins>SIMcheck>system calibration, "Illumination Pattern Focus", SIMcheck.Cal_PatternFocus
Plugins>SIMcheck>utilities, "Format Converter", SIMcheck.Util_FormatConverter
Plugins>SIMcheck>utilities, "Raw SI Data to Pseudo-Widefield", SIMcheck.Util_SItoPseudoWidefield
Plugins>SIMcheck>utilities, "Rescale to 16-bit (discard negative)", SIMcheck.Util_RescaleTo16bit
Plugins>SIMcheck>utilities, "Stack FFT (2D)", SIMcheck.Util_StackFFT2D
Image>Lookup Tables, "SIMcheck MCNR", MCNR.lut
Image>Lookup Tables, "SIMcheck Fourier", SIMcheckFourier.lut
